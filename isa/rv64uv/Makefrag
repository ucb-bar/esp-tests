	vvmul_d \
	overlap \
	sched_sreg_xbar sched_fadd sched_waw sched_war \
	pointer \
	vcjal vfirst vfence \
	cmp fcmp \
	vmerge \
	vclipb vcliph vclipw \
	vmadd \
	vredsum \

rv64uv_sc_vec_tests = \
	 \

rv64uv_p_tests = $(addprefix rv64uv-p-, $(rv64uv_sc_tests))
rv64uv_v_tests = $(addprefix rv64uv-v-, $(filter-out vsetcfgi vsetcfg vsetvl,$(rv64uv_sc_tests))) #FIXME: fix long tests in v env
rv64uv_vp_tests = $(addprefix rv64uv-vp-, $(filter-out vsetcfgi vsetcfg vsetvl,$(rv64uv_sc_tests))) #FIXME: fix long tests in v env

spike_tests += $(rv64uv_p_tests) $(rv64uv_vp_tests) $(rv64uv_v_tests)
